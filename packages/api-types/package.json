{
  "name": "@synap-core/api-types",
  "version": "1.1.4",
  "description": "Type definitions for Synap API Router - tRPC types for frontend",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc && cp src/generated.d.ts dist/generated.d.ts",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@synap-core/types": "^1.2.14",
    "yjs": "^13.0.0"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.55.2",
    "@trpc/server": "^11.8.1",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@trpc/server": "^11.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/synap/synap.git",
    "directory": "packages/api-types"
  },
  "keywords": [
    "synap",
    "api",
    "types",
    "trpc",
    "router"
  ],
  "license": "MIT"
}
