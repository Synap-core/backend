{
  "name": "@initiativ/core",
  "version": "0.1.0",
  "description": "Core orchestrator for Initiativ - brings all packages together",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "orchestration",
    "workflows",
    "chat-branching",
    "knowledge-management"
  ],
  "author": "Antoine Servant",
  "license": "MIT",
  "dependencies": {
    "@initiativ/input": "workspace:*",
    "@initiativ/storage": "workspace:*",
    "@initiativ/rag": "workspace:*",
    "@initiativ/memory": "workspace:*",
    "@initiativ/agents": "workspace:*",
    "@initiativ/git": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "typescript": "^5.7.2"
  }
}

