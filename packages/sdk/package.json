{
  "name": "@synap-core/sdk",
  "version": "0.2.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "High-level SDK for Synap - Event-sourced personal data platform",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsc -p tsconfig.json --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "synap",
    "sdk",
    "event-sourcing",
    "personal-data",
    "knowledge-graph"
  ],
  "author": "Synap Team",
  "license": "AGPL-3.0",
  "dependencies": {
    "@synap-core/client": "^0.1.0",
    "@synap-core/types": "^0.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.3"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/synap-core/synap-backend",
    "directory": "packages/sdk"
  },
  "homepage": "https://github.com/synap-core/synap-backend#readme"
}