{
  "name": "@synap/client",
  "version": "0.1.0",
  "description": "Type-safe client SDK for Synap Core OS",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "import": "./dist/react.mjs",
      "require": "./dist/react.js",
      "types": "./dist/react.d.ts"
    },
    "./realtime": {
      "import": "./dist/realtime.mjs",
      "require": "./dist/realtime.js",
      "types": "./dist/realtime.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "pnpm build:js && pnpm build:types",
    "build:js": "tsup src/index.ts src/react.ts src/realtime.ts --format cjs,esm",
    "build:types": "tsc --project tsconfig.json --emitDeclarationOnly --declaration --outDir dist",
    "dev": "tsup src/index.ts --watch --format esm",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "@trpc/client": "^11.0.0",
    "zod": "^3.22.0"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^5.0.0",
    "@trpc/react-query": "^11.0.0",
    "react": "^18.0.0"
  },
  "devDependencies": {
    "@synap/api": "workspace:*",
    "@trpc/react-query": "^11.0.0",
    "@trpc/server": "^11.0.0",
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "synap",
    "trpc",
    "client",
    "sdk",
    "knowledge-management",
    "ai"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Synap-core/backend-os"
  }
}