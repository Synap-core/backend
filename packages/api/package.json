{
  "name": "@synap/api",
  "version": "1.0.2",
  "description": "Synap API Router - tRPC types and router for Data Pod",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "export $(grep -v '^#' ../../.env | xargs) && tsx watch src/index.ts",
    "start": "export $(grep -v '^#' ../../.env | xargs) && tsx src/index.ts",
    "build": "rm -rf dist && tsc -p tsconfig.types.json && tsup && pnpm gen-types",
    "gen-types": "node scripts/gen-types.mjs",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.926.0",
    "@hono/trpc-server": "^0.3.2",
    "@synap-core/core": "workspace:*",
    "@synap-core/hub-protocol": "workspace:*",
    "@synap-core/types": "workspace:*",
    "@synap/ai": "workspace:*",
    "@synap/ai-embeddings": "workspace:*",
    "@synap/auth": "workspace:*",
    "@synap/database": "workspace:*",
    "@synap/events": "workspace:*",
    "@synap/jobs": "workspace:*",
    "@synap/search": "workspace:*",
    "@synap/storage": "workspace:*",
    "@trpc/server": "^11.8.1",
    "@types/ws": "^8.18.1",
    "drizzle-orm": "^0.45.1",
    "drizzle-zod": "0.7.0",
    "inngest": "3.22.12",
    "jsonwebtoken": "^9.0.2",
    "postgres": "^3.4.8",
    "superjson": "^2.2.6",
    "ws": "^8.18.3",
    "yjs": "^13.6.29",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@trpc/react-query": "^11.8.1",
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/react": "^19.2.8",
    "bcrypt": "^6.0.0",
    "dts-bundle-generator": "^9.5.1",
    "react": "^19.2.3",
    "tsup": "^8.0.0",
    "typescript": "^5.9.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/synap/synap.git",
    "directory": "packages/api"
  },
  "keywords": [
    "synap",
    "api",
    "trpc",
    "router"
  ],
  "license": "MIT"
}
