{
  "name": "@synap/api",
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "export $(grep -v '^#' ../../.env | xargs) && tsx watch src/index.ts",
    "start": "export $(grep -v '^#' ../../.env | xargs) && tsx src/index.ts",
    "build": "tsc -p tsconfig.json"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.926.0",
    "@hono/trpc-server": "^0.3.2",
    "@synap/ai": "workspace:*",
    "@synap/ai-embeddings": "workspace:*",
    "@synap/auth": "workspace:*",
    "@synap-core/core": "workspace:*",
    "@synap-core/database": "workspace:*",
    "@synap/events": "workspace:*",
    "@synap/hub-protocol": "workspace:*",
    "@synap/jobs": "workspace:*",
    "@synap/storage": "workspace:*",
    "@synap-core/types": "workspace:^",
    "@trpc/server": "^11.0.0",
    "@types/ws": "^8.18.1",
    "inngest": "^3.15.0",
    "jsonwebtoken": "^9.0.2",
    "ws": "^8.18.3",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "bcrypt": "^6.0.0",
    "typescript": "^5.3.3"
  },
  "publishConfig": {
    "access": "public"
  }
}