{
  "name": "@synap-core/types",
  "version": "0.3.6",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./entities": {
      "types": "./dist/entities/index.d.ts",
      "import": "./dist/entities/index.js"
    },
    "./documents": {
      "types": "./dist/documents/index.d.ts",
      "import": "./dist/documents/index.js"
    },
    "./users": {
      "types": "./dist/users/index.d.ts",
      "import": "./dist/users/index.js"
    },
    "./inbox": {
      "types": "./dist/inbox/index.d.ts",
      "import": "./dist/inbox/index.js"
    },
    "./workspaces": {
      "types": "./dist/workspaces/index.d.ts",
      "import": "./dist/workspaces/index.js"
    },
    "./views": {
      "types": "./dist/views/index.d.ts",
      "import": "./dist/views/index.js"
    },
    "./relations": {
      "types": "./dist/relations/index.d.ts",
      "import": "./dist/relations/index.js"
    },
    "./preferences": {
      "types": "./dist/preferences/index.d.ts",
      "import": "./dist/preferences/index.js"
    },
    "./realtime": {
      "types": "./dist/realtime/index.d.ts",
      "import": "./dist/realtime/index.js"
    },
    "./events": {
      "types": "./dist/events/index.d.ts",
      "import": "./dist/events/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "zod": "^3.25.0",
    "yjs": "^13.6.11"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.6.0",
    "@synap/database": "workspace:*"
  }
}