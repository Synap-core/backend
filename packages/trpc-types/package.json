{
  "name": "@synap/trpc-types",
  "version": "0.1.0",
  "description": "Shared tRPC types for Synap frontend/backend",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --clean",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@synap/api": "file:../api"
  },
  "peerDependencies": {
    "@trpc/client": "^11.0.0",
    "@trpc/react-query": "^11.0.0"
  },
  "devDependencies": {
    "@trpc/client": "^11.0.0",
    "@trpc/react-query": "^11.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.9.0"
  }
}